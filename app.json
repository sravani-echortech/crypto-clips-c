{
  "expo": {
    "name": "CryptoClips",
    "slug": "crypto-clips",
    "owner": "subscriptions-echor",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.crypto-clips.app",
      "infoPlist": {
        "NSUserTrackingUsageDescription": "This app uses tracking to provide personalized news recommendations.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.crypto-clips.app",
      "permissions": [
        "NOTIFICATIONS",
        "VIBRATE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0A0E27"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "cryptoclips"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "plugins": [
      "expo-notifications",
      "expo-font",
      "expo-localization",
      "expo-web-browser",
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "cryptoclips",
          "organization": "cygnus-digital-innovations"
        }
      ]
    ],
    "scheme": "cryptoclips",
    "extra": {
      "eas": {
        "projectId": "f9382ab8-dda8-4e41-9c9b-45887d9ef630"
      }
    }
  }
}
